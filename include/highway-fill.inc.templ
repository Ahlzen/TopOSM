    <Filter>[highway] = 'tertiary' and [tunnel] = 'yes'</Filter>
    <LineSymbolizer stroke-width="0.6" stroke="&smallroadcolorlowzoom;" stroke-dasharray="4,2" stroke-linejoin="round" stroke-linecap="round" />
  </Rule>
  <Rule>
  	&minz10; &maxz11;
    <Filter>[highway] = 'tertiary' and [tunnel] = 'yes'</Filter>
