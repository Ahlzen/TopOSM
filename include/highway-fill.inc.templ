  </Rule>
  <Rule>
  	&minz12; &maxz13;
    <Filter>[highway] = 'tertiary' and [tunnel] = 'yes'</Filter>
    <LineSymbolizer stroke-width="2.0" stroke="&smallroadcolortunnel;" stroke-linejoin="round" stroke-linecap="round" />
  </Rule>
  <Rule>
